#:kivy 1.9.1

<TabPanel>:
  size_hint: (1, None)
  height: self.minimum_height

<TabButton>:
  shorten: True
  shorten_from: 'left'
  size_hint: (None, None)
  width: 200
  height: 25
  text_size: (self.size[0], None)
  halign: 'center'
  selected_background_color: [0.2, 0.2, 0.2, 1]
  deselected_background_color: [0.6, 0.6, 0.6, 1]
